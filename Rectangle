#include <iostream>
#include<cstring>
using namespace std;
class Rectangle 
{
public:
    void setLength(int len)
    {
    	length=len;
	}
    void setWidth(int w)
    {
    	width=w;
	}
    int getArea()
    {
    	return length*width;
	}
private:
    int length, width; 
};
int main()
{
    Rectangle r;
    int len, w;
    cin>>len>> w;
    r.setLength(len);
    r.setWidth(w);
    cout<<r.getArea() << "\n";
    return 0;
}
