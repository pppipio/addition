#include <iostream>
using namespace std;
void  swap(int *pa, int *pb, int *pc)
{
	int  t;
	if (*pa>*pb)
	{
		t = *pa;	*pa = *pb;  *pb = t;
	}
	if (*pa>*pc)
	{
		t = *pa;	*pa = *pc;  *pc = t;
	}
	if (*pb>*pc)
	{
		t = *pb;	*pb = *pc;  *pc = t;
	}
}
int main(void)
{
	int a, b, c;
	cin>> a >> b >> c;
	swap(&a, &b, &c);
	cout<< a << '\t' << b << '\t' << c << '\n';
	return 0;
}
